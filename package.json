{
  "name": "skeleton-templates",
  "version": "0.0.1",
  "description": "Production ready componenents with Skeleton UI",
  "author": "@imyt <aditya.musunuri@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "postinstall": "npm run check && npm run build-component-index",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "create-component": "esno scripts/create-component",
    "build-component-index": "esno scripts/build-component-index"
  },
  "devDependencies": {
    "@clack/prompts": "0.6.3",
    "@floating-ui/dom": "1.2.1",
    "@iconify/json": "2.2.27",
    "@playwright/test": "1.31.1",
    "@skeletonlabs/skeleton": "0.132.5",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/adapter-vercel": "2.1.1",
    "@sveltejs/kit": "1.15.2",
    "@tailwindcss/forms": "0.5.3",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "@vercel/analytics": "0.1.10",
    "autoprefixer": "10.4.13",
    "chalk": "5.2.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-svelte3": "4.0.0",
    "esno": "0.16.3",
    "postcss": "8.4.31",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.4",
    "prettier-plugin-svelte": "2.9.0",
    "svelte": "3.55.1",
    "svelte-check": "3.0.4",
    "svelte-highlight": "7.2.0",
    "svelte-loader": "3.1.6",
    "svelte-preprocess": "5.0.1",
    "tailwindcss": "3.2.7",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "unplugin-icons": "0.15.3",
    "vite": "4.1.5",
    "vitest": "0.29.1"
  },
  "type": "module"
}