{
  "name": "skeleton-templates",
  "version": "0.0.1",
  "description": "Production ready componenents with Skeleton UI",
  "author": "@imyt <aditya.musunuri@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "postinstall": "npm run check && npm run build-component-index",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "create-component": "esno scripts/create-component",
    "build-component-index": "esno scripts/build-component-index"
  },
  "devDependencies": {
    "@clack/prompts": "0.7.0",
    "@floating-ui/dom": "1.5.3",
    "@iconify/json": "2.2.128",
    "@playwright/test": "1.39.0",
    "@skeletonlabs/skeleton": "2.3.0",
    "@skeletonlabs/tw-plugin": "0.2.2",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/adapter-vercel": "3.0.3",
    "@sveltejs/kit": "1.25.2",
    "@tailwindcss/forms": "0.5.6",
    "@types/node": "20.8.6",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vercel/analytics": "1.1.1",
    "autoprefixer": "10.4.16",
    "chalk": "5.3.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte3": "4.0.0",
    "esno": "0.17.0",
    "postcss": "8.4.31",
    "postcss-load-config": "4.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "svelte-highlight": "7.4.1",
    "svelte-loader": "3.1.9",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "unplugin-icons": "0.17.0",
    "vite": "4.5.2",
    "vite-plugin-tailwind-purgecss": "0.1.3",
    "vitest": "0.34.6"
  },
  "type": "module"
}
